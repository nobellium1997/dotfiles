set number
set ignorecase
set incsearch
set relativenumber
set backspace=2
set wrap
syntax on

colorscheme slate

noremap! <C-BS> <C-w>
noremap! <C-h> <C-w>
noremap! <C-V> <esc>"+gPi

nnoremap <space> i<space><esc>l
nnoremap <return> o<esc>
nnoremap <S-return> O<esc>
nnoremap J <C-e>
nnoremap K <C-y>
nnoremap <C-[> <C-u>
nnoremap <C-]> <C-d> 

vnoremap <C-C> "+y
map <C-V> "+gP

autocmd GUIEnter * simalt ~x
set guioptions+=b 
set guifont=Lucida\ Console:h12

call plug#begin('~/.vim/plugged')

Plug 'tomasiser/vim-code-dark'

call plug#end()
